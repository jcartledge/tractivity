body
  padding: 50px
  font: 14px Helvetica, Arial, sans-serif

a
  display: block
  color: #00B7FF
  font-size: 40px
  font-weight: bold;
  text-decoration: none;
  &:hover
    text-decoration: underline;
small
  display: block
  text-align: right
  font-size: 14px
  color: silver
  margin-top: 1em
  font-weight: bold
li
  margin: 0
  list-style-type: none;
  border-bottom: 1px solid #EEE;
